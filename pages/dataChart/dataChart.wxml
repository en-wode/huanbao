      <view class="zan-cell">
        <view class="zan-cell__bd" >日期选择:</view>
          <view class="zan-cell__ft" bindtap='message'>
            <view class="demo__icon zan-badge">
            {{dates}} 
            </view>
        </view>
      </view>
      <!-- <view class="picker">  
        日期:  
      </view>   -->
    </picker>  
  </view>  
